{
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "https://api.bithuman.ai/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/docs",
      "permanent": false,
      "has": [
        {
          "type": "query",
          "key": "_docsify"
        }
      ]
    }
  ]
}
